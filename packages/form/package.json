    "@pluralsight/ps-design-system-switch": "^6.0.11",
    "@pluralsight/ps-design-system-tag": "^7.0.12",
    "@pluralsight/ps-design-system-text": "^16.0.15",
    "@pluralsight/ps-design-system-textarea": "^7.0.26",
    "@pluralsight/ps-design-system-textinput": "^8.0.8",
    "@pluralsight/ps-design-system-theme": "^4.0.11"
  }
