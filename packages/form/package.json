  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^4.0.0",
    "glamor": "^2.20.40",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-actionmenu": "^13.0.11",
    "@pluralsight/ps-design-system-build": "^1.11.15",
    "@pluralsight/ps-design-system-button": "^18.0.17",
