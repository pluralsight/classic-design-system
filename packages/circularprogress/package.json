{
  "name": "@pluralsight/ps-design-system-circularprogress",
  "version": "8.1.5",
  "description": "Design System component for circular progress ui",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "author": "jaketrent",
  "keywords": [],
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "run-s build:esm build:css",
    "build:css": "build-css --useGlamor -i dist/esm/css/index.js",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:js --watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsc --noEmit --project ./tsconfig.json && jest",
    "test:watch": "yarn test --watchAll"
  },
  "sideEffects": false,
  "style": "dist/css/index.css",
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^6.9.3",
    "@pluralsight/ps-design-system-screenreaderonly": "^3.1.5",
    "@pluralsight/ps-design-system-util": "^7.2.1"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^7.0.0",
    "glamor": "^2.20.40",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^2.1.0",
    "@pluralsight/ps-design-system-storybook-preset": "^0.4.4",
    "@pluralsight/ps-design-system-theme": "^7.1.4"
  }
}
