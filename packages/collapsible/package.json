{
  "name": "@pluralsight/ps-design-system-collapsible",
  "version": "2.1.2",
  "description": "Collapsible UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:js build:cjs",
    "build:cjs": "babel --root-mode upward src --out-dir dist/cjs",
    "build:js": "cross-env ESM=true babel --root-mode upward src --out-dir dist/esm",
    "build:watch": "yarn build:js -- --watch",
    "prepublish": "yarn build",
    "test": "jest",
    "test:watch": "yarn test -- --watchAll"
  },
  "dependencies": {
    "@pluralsight/ps-design-system-filter-react-props": "^1.1.18",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^1.11.16"
  }
}
