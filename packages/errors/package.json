{
  "name": "@pluralsight/ps-design-system-errors",
  "version": "7.0.5",
  "description": "Design system components for errors",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "author": "jaketrent",
  "keywords": [],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "run-s build:svg build:js build:cjs build:css",
    "build:cjs": "babel --root-mode upward src --out-dir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:js": "cross-env ESM=true babel --root-mode upward src --out-dir dist/esm",
    "build:svg": "svgr --filename-case kebab --ext \"dist.js\" -d src/react/icons src/svg",
    "build:watch": "npm run build:js -- --watch",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "dependencies": {
    "@pluralsight/ps-design-system-button": "^16.0.5",
    "@pluralsight/ps-design-system-core": "^5.0.2",
    "@pluralsight/ps-design-system-icon": "^15.1.1",
    "@pluralsight/ps-design-system-text": "^14.0.6",
    "camelize": "^1.0.0",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^3.1.7",
    "@pluralsight/ps-design-system-theme": "^4.0.0",
    "glamor": "^2.20.40",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^1.11.3",
    "@pluralsight/ps-design-system-normalize": "^3.1.9",
    "@pluralsight/ps-design-system-storybook-addon-theme": "^5.0.3",
    "@pluralsight/ps-design-system-theme": "^4.0.2"
  }
}
