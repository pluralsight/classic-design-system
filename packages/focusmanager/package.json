{
  "name": "@pluralsight/ps-design-system-focusmanager",
  "version": "8.0.5",
  "description": "FocusManager UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "src/*",
    "dist/*",
    "!src/*/__stories__/*",
    "!src/*/__specs__/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:esm build:cjs",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:watch": "yarn build:esm --watch",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "storybook": "start-storybook -p 6006",
    "test": "tsc --noEmit --project ./tsconfig.json && jest",
    "test:watch": "yarn test --watchAll"
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "*",
    "react": "*"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^4.0.0",
    "@pluralsight/ps-design-system-button": "^24.0.6",
    "@pluralsight/ps-design-system-storybook-preset": "^0.7.12",
    "@pluralsight/ps-design-system-text": "^20.1.24"
  },
  "gitHead": "0c5576456d80ae8d6dfb82da74320f3e8266c35a"
}
