{
  "name": "@pluralsight/ps-design-system-storybook-addon-theme",
  "version": "8.2.1",
  "description": "Storybook addon for Theme switching",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "author": "jaketrent",
  "keywords": [
    "react-storybook",
    "storybook-addon",
    "style"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json --module commonjs --target es5",
    "build:watch": "yarn build --watch",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^7.0.0",
    "@pluralsight/ps-design-system-theme": "^7.1.8",
    "@storybook/addons": "^6.0.26",
    "@storybook/api": "^6.0.26",
    "@storybook/client-logger": "^6.0.26",
    "@storybook/components": "^6.0.26",
    "@storybook/core-events": "^6.0.26",
    "@storybook/theming": "^6.0.26"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/cjs/index.js"
  }
}
