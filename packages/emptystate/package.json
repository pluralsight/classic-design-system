{
  "name": "@pluralsight/ps-design-system-emptystate",
  "version": "8.1.7",
  "description": "EmptyState UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:svg build:cjs build:esm build:css copy:svg",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:svg": "svgr --typescript --filename-case kebab --ext \"dist.tsx\" -d src/react/illustrations src/svg",
    "build:watch": "yarn build:esm -- --watch",
    "copy:svg": "node copy-svgs.js",
    "prepublish": "yarn build",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "yarn test -- --watchAll"
  },
  "style": "dist/index.css",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^6.4.3",
    "@pluralsight/ps-design-system-util": "^5.3.0"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^4.0.0",
    "glamor": "^2.x.x",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^1.11.17",
    "@pluralsight/ps-design-system-button": "^18.2.0",
    "@pluralsight/ps-design-system-icon": "^18.2.4",
    "@pluralsight/ps-design-system-storybook-preset": "^0.1.3",
    "@pluralsight/ps-design-system-tag": "^7.0.18",
    "@pluralsight/ps-design-system-textinput": "^8.1.7",
    "@pluralsight/ps-design-system-theme": "^4.0.14",
    "ncp": "^2.0.0"
  }
}
