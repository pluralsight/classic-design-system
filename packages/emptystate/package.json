{
  "name": "@pluralsight/ps-design-system-emptystate",
  "version": "8.0.1",
  "description": "EmptyState UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:svg build:js build:cjs build:css copy:svg",
    "build:cjs": "babel --root-mode upward src --out-dir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:js": "cross-env ESM=true babel --root-mode upward src --out-dir dist/esm",
    "build:svg": "svgr --filename-case kebab --ext \"dist.js\" -d src/react/illustrations src/svg",
    "build:watch": "npm run build:js -- --watch",
    "copy:svg": "node copy-svgs.js",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^6.0.1",
    "@pluralsight/ps-design-system-filter-react-props": "^1.1.17",
    "@pluralsight/ps-design-system-prop-types": "^2.0.18",
    "prop-types": "^15.7.2",
    "resize-observer-polyfill": "^1.5.1"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^3.1.7",
    "@pluralsight/ps-design-system-theme": "^4.0.0",
    "glamor": "^2.x.x",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^1.11.13",
    "@pluralsight/ps-design-system-button": "^18.0.1",
    "@pluralsight/ps-design-system-icon": "^18.0.1",
    "@pluralsight/ps-design-system-storybook-addon-center": "^2.1.10",
    "@pluralsight/ps-design-system-storybook-addon-theme": "^6.0.1",
    "@pluralsight/ps-design-system-tag": "^7.0.1",
    "@pluralsight/ps-design-system-textinput": "^7.0.1",
    "@pluralsight/ps-design-system-theme": "^4.0.10",
    "ncp": "^2.0.0"
  }
}
