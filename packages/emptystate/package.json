{
  "name": "@pluralsight/ps-design-system-emptystate",
  "version": "12.0.4",
  "description": "EmptyState UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:svg build:esm build:css copy:svg",
    "build:css": "build-css --useGlamor -i dist/esm/css/index.js",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:svg": "node scripts/generate-icons.js",
    "oldbuild:svg": "svgr --typescript --filename-case kebab --ext \"dist.tsx\" -d src/react/illustrations src/svg",
    "build:watch": "yarn build:esm --watch",
    "copy:svg": "node copy-svgs.js",
    "storybook": "start-storybook -p 6006",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "yarn test --watchAll"
  },
  "style": "dist/index.css",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^6.9.3",
    "@pluralsight/ps-design-system-util": "^7.2.1"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^7.0.0",
    "glamor": "^2.x.x",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^2.1.0",
    "@pluralsight/ps-design-system-button": "^21.0.4",
    "@pluralsight/ps-design-system-icon": "^21.0.4",
    "@pluralsight/ps-design-system-storybook-preset": "^0.4.4",
    "@pluralsight/ps-design-system-tag": "^9.0.4",
    "@pluralsight/ps-design-system-textinput": "^10.1.5",
    "@pluralsight/ps-design-system-theme": "^7.1.4",
    "ncp": "^2.0.0"
  }
}
