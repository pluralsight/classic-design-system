  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "*",
    "glamor": "^2.x.x",
    "react": ">=16.3.0 < 17.0.0"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^1.10.1",
    "@pluralsight/ps-design-system-storybook-addon-center": "^2.0.5",
