{
  "name": "@pluralsight/ps-design-system-verticaltabs",
  "version": "4.2.24",
  "description": "Verticaltab UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "run-s build:cjs build:esm build:css",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:cjs --watch",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "yarn test --watchAll"
  },
  "style": "dist/index.css",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-collapsible": "^3.0.5",
    "@pluralsight/ps-design-system-core": "^6.7.2",
    "@pluralsight/ps-design-system-icon": "^19.0.21",
    "@pluralsight/ps-design-system-util": "^6.0.10"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^6.0.0",
    "glamor": "^2.20.40",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^2.0.3",
    "@pluralsight/ps-design-system-storybook-preset": "^0.2.23",
    "@pluralsight/ps-design-system-theme": "^6.0.15"
  }
}
