{
  "name": "@pluralsight/ps-design-system-verticaltabs",
  "version": "7.1.14",
  "description": "Verticaltab UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "src/*",
    "dist/*",
    "!src/*/__stories__/*",
    "!src/*/__specs__/*"
  ],
  "scripts": {
    "build": "run-s build:esm build:cjs build:css",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:cjs --watch",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "yarn test --watchAll"
  },
  "sideEffects": false,
  "style": "dist/index.css",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-collapsible": "^5.1.4",
    "@pluralsight/ps-design-system-core": "^8.1.4",
    "@pluralsight/ps-design-system-icon": "^23.1.9",
    "@pluralsight/ps-design-system-util": "^9.2.3"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^5.1.2",
    "@pluralsight/ps-design-system-theme": "^8.1.4",
    "glamor": "^2.20.40",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^3.1.3",
    "@pluralsight/ps-design-system-storybook-preset": "^0.6.7",
    "@pluralsight/ps-design-system-theme": "^8.1.7"
  },
  "gitHead": "604e0e18fef14a0008380eb5ecfbbb70448fc5a5"
}
