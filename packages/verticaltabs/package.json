{
  "name": "@pluralsight/ps-design-system-verticaltabs",
  "version": "8.0.4",
  "description": "Verticaltab UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "react.d.ts",
    "react.mjs",
    "react.js",
    "styles.css",
    "src/*",
    "dist/*",
    "!src/*/__stories__/*",
    "!src/*/__specs__/*"
  ],
  "scripts": {
    "build": "run-s build:esm build:cjs build:css",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:css": "postcss-compile",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:cjs --watch",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "yarn test --watchAll"
  },
  "sideEffects": [
    "**/*.css",
    "src/index.ts"
  ],
  "style": "dist/esm/css/index.css",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-collapsible": "^5.1.10",
    "@pluralsight/ps-design-system-core": "^9.0.1",
    "@pluralsight/ps-design-system-icon": "^24.1.1",
    "@pluralsight/ps-design-system-util": "^9.3.1"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^6.0.0",
    "@pluralsight/ps-design-system-theme": "^8.1.4",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^4.0.0",
    "@pluralsight/ps-design-system-storybook-preset": "^0.7.6",
    "@pluralsight/ps-design-system-theme": "^8.1.16"
  },
  "gitHead": "0c5576456d80ae8d6dfb82da74320f3e8266c35a"
}