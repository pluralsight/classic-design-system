{
  "name": "@pluralsight/ps-design-system-navitem",
  "version": "5.1.12",
  "description": "NavItem UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "src/*",
    "dist/*",
    "!src/*/__stories__/*",
    "!src/*/__specs__/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:esm build:cjs build:css",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:esm --watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsc --noEmit --project ./tsconfig.json && jest",
    "test:watch": "yarn test --watchAll"
  },
  "sideEffects": false,
  "style": "dist/index.css",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^8.2.0",
    "@pluralsight/ps-design-system-halo": "^10.1.10",
    "@pluralsight/ps-design-system-theme": "^8.1.10",
    "@pluralsight/ps-design-system-util": "^9.2.6"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^5.1.2",
    "@pluralsight/ps-design-system-theme": "^8.1.4",
    "glamor": "^2.x.x",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-actionmenu": "^17.1.12",
    "@pluralsight/ps-design-system-build": "^3.2.0",
    "@pluralsight/ps-design-system-button": "^22.1.12",
    "@pluralsight/ps-design-system-icon": "^23.1.12",
    "@pluralsight/ps-design-system-position": "^8.1.12",
    "@pluralsight/ps-design-system-storybook-preset": "^0.7.0"
  },
  "gitHead": "287df3d6daa2044fe511c370ad581bfa529c67db"
}
