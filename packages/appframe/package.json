{
  "name": "@pluralsight/ps-design-system-appframe",
  "version": "5.1.16",
  "description": "AppFrame UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "src/*",
    "dist/*",
    "!src/*/__stories__/*",
    "!src/*/__specs__/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:esm build:cjs build:css",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:esm --watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsc --noEmit --project ./tsconfig.json && jest",
    "test:watch": "yarn test --watchAll"
  },
  "sideEffects": false,
  "style": "dist/index.css",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-button": "^22.1.9",
    "@pluralsight/ps-design-system-core": "^8.1.4",
    "@pluralsight/ps-design-system-scrollable": "^5.1.9",
    "@pluralsight/ps-design-system-util": "^9.2.3",
    "body-scroll-lock": "^3.0.3",
    "exenv": "^1.2.2",
    "focus-within": "^3.0.2"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^5.1.2",
    "@pluralsight/ps-design-system-theme": "^8.1.4",
    "glamor": "^2.x.x",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^3.1.3",
    "@pluralsight/ps-design-system-button": "*",
    "@pluralsight/ps-design-system-icon": "^23.1.9",
    "@pluralsight/ps-design-system-layout": "^11.1.10",
    "@pluralsight/ps-design-system-navbar": "^5.1.9",
    "@pluralsight/ps-design-system-navbrand": "^4.1.7",
    "@pluralsight/ps-design-system-navitem": "^5.1.9",
    "@pluralsight/ps-design-system-navuser": "^4.1.9",
    "@pluralsight/ps-design-system-storybook-preset": "^0.6.7",
    "@pluralsight/ps-design-system-text": "^20.1.8",
    "@pluralsight/ps-design-system-theme": "^8.1.7",
    "@pluralsight/ps-design-system-verticaltabs": "^7.1.14",
    "@types/body-scroll-lock": "^2.6.1"
  },
  "gitHead": "604e0e18fef14a0008380eb5ecfbbb70448fc5a5"
}
