{
  "name": "@pluralsight/ps-design-system-appframe",
  "version": "1.1.20",
  "description": "AppFrame UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:js build:cjs build:css",
    "build:cjs": "babel --root-mode upward src --out-dir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:js": "cross-env ESM=true babel --root-mode upward src --out-dir dist/esm",
    "build:watch": "yarn build:js -- --watch",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "yarn test -- --watchAll"
  },
  "gitHead": "7418883d96a76c59a99f86c2b16516ec752bf913",
  "dependencies": {
    "@pluralsight/ps-design-system-button": "^18.2.2",
    "@pluralsight/ps-design-system-core": "^6.4.4",
    "@pluralsight/ps-design-system-filter-react-props": "^1.1.20",
    "@pluralsight/ps-design-system-scrollable": "^1.1.13",
    "@pluralsight/ps-design-system-util": "^5.3.2",
    "body-scroll-lock": "^3.0.3",
    "exenv": "^1.2.2",
    "focus-within": "^3.0.2",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^3.1.4",
    "@pluralsight/ps-design-system-theme": "^3.0.2",
    "glamor": "^2.x.x",
    "react": ">=16.8.6 < 17.0.0"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^1.11.17",
    "@pluralsight/ps-design-system-button": "*",
    "@pluralsight/ps-design-system-icon": "^18.2.6",
    "@pluralsight/ps-design-system-layout": "^7.0.22",
    "@pluralsight/ps-design-system-navbar": "^1.1.25",
    "@pluralsight/ps-design-system-navbrand": "^1.1.15",
    "@pluralsight/ps-design-system-navitem": "^1.3.16",
    "@pluralsight/ps-design-system-navuser": "^1.1.16",
    "@pluralsight/ps-design-system-storybook-preset": "^0.1.5",
    "@pluralsight/ps-design-system-text": "^16.0.26",
    "@pluralsight/ps-design-system-theme": "^4.0.15",
    "@pluralsight/ps-design-system-verticaltabs": "^4.1.3"
  }
}
