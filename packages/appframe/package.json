{
  "name": "@pluralsight/ps-design-system-appframe",
  "version": "6.0.13",
  "description": "AppFrame UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "src/*",
    "dist/*",
    "!src/*/__stories__/*",
    "!src/*/__specs__/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:esm build:cjs build:css",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:css": "postcss-compile",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:esm --watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsc --noEmit --project ./tsconfig.json && jest",
    "test:watch": "yarn test --watchAll"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "style": "dist/esm/css/index.css",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-button": "^24.0.6",
    "@pluralsight/ps-design-system-core": "^10.0.2",
    "@pluralsight/ps-design-system-scrollable": "^6.0.8",
    "@pluralsight/ps-design-system-util": "^10.1.1",
    "body-scroll-lock": "^3.0.3",
    "exenv": "^1.2.2",
    "focus-within": "^3.0.2"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "*",
    "@pluralsight/ps-design-system-theme": "*",
    "react": "*"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^4.0.0",
    "@pluralsight/ps-design-system-button": "*",
    "@pluralsight/ps-design-system-icon": "^25.1.2",
    "@pluralsight/ps-design-system-layout": "^12.0.10",
    "@pluralsight/ps-design-system-navbar": "^6.0.12",
    "@pluralsight/ps-design-system-navbrand": "^5.0.8",
    "@pluralsight/ps-design-system-navitem": "^6.0.12",
    "@pluralsight/ps-design-system-navuser": "^5.0.12",
    "@pluralsight/ps-design-system-storybook-preset": "^0.7.12",
    "@pluralsight/ps-design-system-text": "^20.1.24",
    "@pluralsight/ps-design-system-theme": "^8.1.22",
    "@pluralsight/ps-design-system-verticaltabs": "^8.0.11",
    "@types/body-scroll-lock": "^2.6.1"
  },
  "gitHead": "0c5576456d80ae8d6dfb82da74320f3e8266c35a"
}
