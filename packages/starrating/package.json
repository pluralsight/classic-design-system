{
  "name": "@pluralsight/ps-design-system-starrating",
  "version": "1.2.3",
  "description": "Design System component for star rating ui",
  "main": "index.js",
  "module": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:js build:css",
    "build:css": "build-css --useGlamor",
    "build:js": "babel src --out-dir dist --ignore spec.js --copy-files",
    "build:watch": "npm run build:js -- --watch",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:updateSnapshot": "npm run test -- --updateSnapshot",
    "test:watch": "npm run test -- --watchAll"
  },
  "keywords": [],
  "author": "travisbumgarner",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "file:../core",
    "@pluralsight/ps-design-system-filter-react-props": "file:../filter-react-props",
    "@pluralsight/ps-design-system-icon": "file:../icon",
    "@pluralsight/ps-design-system-theme": "file:../theme",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^3.0.24",
    "react": ">=0.15.0 < 17.0.0"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-storybook-addon-center": "file:../storybook-addon-center",
    "@pluralsight/ps-design-system-storybook-addon-theme": "file:../storybook-addon-theme"
  }
