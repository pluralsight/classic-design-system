    "@pluralsight/ps-design-system-halo": "^7.1.10",
    "@pluralsight/ps-design-system-icon": "^19.0.8",
    "@pluralsight/ps-design-system-util": "^6.0.1",
    "exenv": "^1.2.2"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^6.0.0",
    "glamor": "^2.20.40",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^1.12.1",
    "@pluralsight/ps-design-system-storybook-preset": "^0.2.10",
