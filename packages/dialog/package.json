{
  "name": "@pluralsight/ps-design-system-dialog",
  "version": "8.4.7",
  "description": "Design System component for dialog ui",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "author": "jaketrent",
  "keywords": [],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "run-s build:cjs build:esm build:css",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:esm -- --watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsc --noEmit --project ./tsconfig.json && jest",
    "test:watch": "yarn test -- --watchAll"
  },
  "style": "dist/index.css",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^6.5.1",
    "@pluralsight/ps-design-system-filter-react-props": "^1.2.1",
    "@pluralsight/ps-design-system-focusmanager": "^5.2.6",
    "@pluralsight/ps-design-system-util": "^6.0.0",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^6.0.0",
    "glamor": "^2.20.40",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^1.12.1",
    "@pluralsight/ps-design-system-button": "^19.0.7",
    "@pluralsight/ps-design-system-storybook-preset": "^0.2.9",
    "@pluralsight/ps-design-system-text": "^16.1.9",
    "@pluralsight/ps-design-system-theme": "^6.0.5"
  }
}
