{
  "name": "@pluralsight/ps-design-system-navbrand",
  "version": "1.2.1",
  "description": "Navbrand UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:js build:cjs build:css",
    "build:cjs": "babel --root-mode upward src --out-dir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:js": "cross-env ESM=true babel --root-mode upward src --out-dir dist/esm",
    "build:watch": "yarn build:js -- --watch",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "yarn test -- --watchAll"
  },
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^6.5.1",
    "@pluralsight/ps-design-system-filter-react-props": "^1.2.1",
    "@pluralsight/ps-design-system-halo": "^7.1.1",
    "@pluralsight/ps-design-system-util": "^5.4.1",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.8",
    "@pluralsight/ps-design-system-theme": "^4.0.11",
    "glamor": "^2.x.x",
    "react": ">=16.8.6 < 17.0.0"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^1.12.1",
    "@pluralsight/ps-design-system-storybook-preset": "^0.2.1"
  }
}
