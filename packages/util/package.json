{
  "name": "@pluralsight/ps-design-system-util",
  "version": "5.2.0",
  "description": "Runtime dependencies for utilitarian tasks. Analogous to a lodash.",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "author": "jaketrent",
  "keywords": [],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "run-s build:cjs build:esm",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:esm -- --watch",
    "prepublish": "yarn build",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "yarn test -- --watch"
  },
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "exenv": "^1.2.2",
    "prop-types": "^15.7.2",
    "resize-observer-polyfill": "^1.5.1"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^1.11.16",
    "@pluralsight/ps-design-system-position": "^4.1.17",
    "@pluralsight/ps-design-system-storybook-preset": "^0.1.2"
  }
}
